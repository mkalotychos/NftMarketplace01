# ========================================
# DEPLOYMENT CONFIGURATION
# ========================================

# Network RPC URLs (for contract deployment)
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
POLYGON_MUMBAI_RPC_URL=https://polygon-mumbai.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
MAINNET_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Private key for deployment (DO NOT COMMIT!)
PRIVATE_KEY=your_private_key_here

# Block explorer API keys for verification
ETHERSCAN_API_KEY=your_etherscan_api_key
POLYGONSCAN_API_KEY=your_polygonscan_api_key

# ========================================
# FRONTEND CONFIGURATION (VITE_* prefix)
# ========================================

# Alchemy SDK (for NFT data fetching)
VITE_ALCHEMY_API_KEY=your_alchemy_api_key
VITE_ALCHEMY_NETWORK=sepolia

# IPFS / Pinata configuration (for uploading NFT images)
VITE_PINATA_API_KEY=your_pinata_api_key
VITE_PINATA_SECRET_KEY=your_pinata_secret_key
VITE_PINATA_JWT=your_pinata_jwt

# Or use NFT.Storage (alternative to Pinata)
VITE_NFT_STORAGE_API_KEY=your_nft_storage_api_key

# Contract addresses (filled after deployment)
VITE_NFT_CONTRACT_ADDRESS=
VITE_MARKETPLACE_CONTRACT_ADDRESS=

# Network configuration
VITE_CHAIN_ID=11155111
VITE_NETWORK_NAME=sepolia

